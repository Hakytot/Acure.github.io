/*================ Offcanvas ================*/

#MainContent{
    height: 100%;
    @include transition(transform 0.3s ease-out 0s, all 0.4s ease 0s);    
    width: 100%;
    &:after{        
        content: "";
        display: block;
        height: 100%;
        width: 100%;
        left: 0;        
        @include opacity(0);
        position: fixed;
        visibility: hidden;
        top: 0;
        @include transition(opacity 0s ease 0s, all 0.4s ease 0s);           
        z-index: -10;        
    }
}

body{   
    &.js-drawer-open{
        position: relative;
        &:before{
            content: "";
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            @include opacity(1);
            visibility: visible;
            z-index: 100;
            background: rgba(#000000, .80);
        }
        #MainContent{
            &:after{
                @include opacity(1);
                visibility: visible;
                z-index: 100;
            }
        }
    }
}


.clearfix {
    &:after {
        content: '';
        display: table;
        clear: both;
    }
    *zoom: 1;
}

#NavDrawer{
    .drawer__close{
        top: 0;
    }
}

