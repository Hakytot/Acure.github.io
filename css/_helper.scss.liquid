.clearfix {
    @include clearfix();
}

.visually-hidden {
    @include visually-hidden();
}

.visibility-hidden {
    visibility: hidden;
}

.js-focus-hidden:focus {
    outline: none;
}

.no-js:not(html) {
    display: none;
    .no-js & {
        display: block;
    }
}

.js {
    .no-js & {
        display: none;
    }
}